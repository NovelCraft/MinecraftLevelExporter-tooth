{
    "format_version": 1,
    "tooth": "github.com/NovelCraft/MinecraftLevelExporter-tooth",
    "version": "0.1.0",
    "information": {
        "name": "Minecraft Level Exporter",
        "description": "Export and convert a Minecraft level to NovelCraft level.",
        "author": "Futrime",
        "repository": "github.com/NovelCraft/MinecraftLevelExporter"
    },
    "placement": [
        {
            "source": "exporter.js",
            "destination": "plugins/exporter.js"
        },
        {
            "source": "converter.exe",
            "destination": ".lip/tools/converter/converter.exe"
        },
        {
            "source": "dict.json",
            "destination": "dict.json"
        }
    ],
    "dependencies": {
        "github.com/Tooth-Hub/LiteLoaderBDS": [
            [
                ">=2.10.1"
            ]
        ]
    }
}
